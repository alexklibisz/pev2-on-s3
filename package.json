{
  "name": "pev2-on-s3",
  "version": "0.1.2",
  "private": true,
  "type": "module",
  "scripts": {
    "dev:docker": "docker compose -f dev/docker-compose.yaml up --build",
    "dev:minio": "docker compose -f dev/docker-compose.yaml up minio minio-init -d",
    "dev:server": "tsx watch --env-file dev/.env server/index.ts",
    "dev:client": "vite --config client/vite.config.ts",
    "build:client": "vite build --config client/vite.config.ts",
    "build:server": "tsc -p tsconfig.json",
    "build": "npm run build:client && npm run build:server",
    "start": "node dist/server/index.js"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.700.0",
    "express": "^4.21.0"
  },
  "devDependencies": {
    "@types/express": "^5.0.0",
    "@types/node": "^25.2.1",
    "@vitejs/plugin-vue": "^6.0.4",
    "bootstrap": "^5.3.3",
    "pev2": "^1.20.1",
    "tsx": "^4.19.0",
    "typescript": "^5.7.0",
    "vite": "^7.3.1",
    "vue": "^3.5.0",
    "vue-router": "^5.0.2"
  }
}
